{
  "manifest_version": 2,
  "name": "Smart Kawaya",
  "version": "0.1",
  "permissions": [
    "storage",
    "https://api.spark.io/v1/devices/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["jquery-2.0.2.min.js"]
    }
  ],
  "options_page": "option.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "vacant_dark.png"
  }
}
